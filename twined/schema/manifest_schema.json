{
  "$defs": {
    "tags": {
      "description": "Key-value tags associated with the object.",
      "type": "object"
    },
    "labels": {
      "description": "Textual labels associated with the object",
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "type": "object",
  "properties": {
    "id": {
      "description": "ID of the manifest, typically a uuid",
      "type": "string"
    },
    "datasets": {
      "type": "object",
      "patternProperties": {
        ".+": {
          "type": "string"
        }
      }
    }
  },
  "required": ["id", "datasets"]
}
